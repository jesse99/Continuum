<Bosses namespaces = "Gear Shared Styler">
	<Extend name = "Application">
		<Implementation interfaces = "IInterface +IFactoryPrefs" name = "FactoryPrefs"/>
	</Extend>
	
	<Boss name = "Stylers" singleton = "true">
		<Implementation interfaces = "IInterface +IFindLanguage" name = "FindLanguage"/>
		<Implementation interfaces = "IWhitespace" name = "Whitespace"/>
	</Boss>
	
	<Boss name = "CsLanguage">
		<Implementation interfaces = "IComputeRuns IStyleWith" name = "CSharpComputer"/>
		<Implementation interfaces = "IDeclarations" name = "CSharpDeclarations"/>
	</Boss>
	
	<Boss name = "RegexLanguage">
		<Implementation interfaces = "IComputeRuns IStyleWith" name = "RegexComputer"/>
		<Implementation interfaces = "IDeclarations" name = "RegexDeclarations"/>
	</Boss>
</Bosses>
