<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE dictionary SYSTEM "file://localhost/System/Library/DTDs/sdef.dtd"><dictionary title="Continuum Terminology" xmlns:xi="http://www.w3.org/2003/XInclude">	<xi:include href="file:///System/Library/ScriptingDefinitions/CocoaStandard.sdef" xpointer="xpointer(/dictionary/suite)"/>		<suite name="Continuum Suite" code="Cont" description="Continuum specific scripting.">		<!-- saveable file format (this is used by the save command declared in CocoaStandard.sdef). -->		<enumeration name="saveable file format" code="savf">			<enumerator name="UTF8" code="UTF8" description="The default file format: utf-8 encoded plain text.">				<cocoa string-value="Plain Text, UTF8 Encoded"/>			</enumerator>			<enumerator name="HTML" code="HTML" description="Saves the formatted document as HTML.">				<cocoa string-value="HTML"/>			</enumerator>			<enumerator name="RTF" code="RTF " description="Saves the formatted document as RTF.">				<cocoa string-value="Rich Text Format (RTF)"/>			</enumerator>			<enumerator name="Word 1997" code="Doc " description="Saves the formatted document using Word 1997 (doc).">				<cocoa string-value="Word 97 Format (doc)"/>			</enumerator>			<enumerator name="Word 2007" code="Docx" description="Saves the formatted document using Word 2007 (docx)">				<cocoa string-value="Word 2007 Format (docx)"/>			</enumerator>			<enumerator name="Open Document" code="Odt " description="Saves the formatted document using Open Document (odt)">				<cocoa string-value="Open Document Text (odt)"/>			</enumerator>		</enumeration>				<!-- commands -->		<command name="debug" code="ContDebg" description="Launches a mono executable using the debugger.">			<cocoa class="DebugCommand"/>			<direct-parameter description="The executable file to launch.">				<type type="text"/>			</direct-parameter>			<parameter name="with" code="with" type="text" optional="yes" description="Command line arguments for the executable.">				<cocoa key="Args"/>			</parameter>			<parameter name="vars" code="vars" type="text" optional="yes" description="Space separated list of environment variables.">				<cocoa key="Env"/>			</parameter>			<parameter name="using" code="use " type="text" optional="yes" description="mono executable to use to launch the app.">				<cocoa key="Using"/>			</parameter>		</command>	</suite></dictionary>